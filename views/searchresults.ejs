<!DOCTYPE html>
<html>
  <head>
    <title>ReviewR</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://s3-media2.fl.yelpcdn.com/assets/srv0/homepage/5a57af5e7478/assets/css/homepage-pkg.css">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600' rel='stylesheet' type='text/css'>
    <link href="https://netdna.bootstrapcdn.com/font-awesome/3.1.1/css/font-awesome.css" rel="stylesheet">
    
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/css/bootstrap.min.css" integrity="sha384-PsH8R72JQ3SOdhVi3uxftmaW6Vc51MKb0q5P2rRUpPvrszuE4W1povHYgTpBfshb" crossorigin="anonymous">
    <link rel="stylesheet" href="https://s3-media2.fl.yelpcdn.com/assets/srv0/homepage/5a57af5e7478/assets/css/homepage-pkg.css">
  </head>
  <body>
    
  <noscript>
    <link rel="stylesheet" href="https://s3-media2.fl.yelpcdn.com/assets/srv0/svg_icons/1421d1203d09/assets/sprite.css">
</noscript>
      
<% include header.ejs %>
<div class="y-container ">
    <div class="y-container_content">
        <div class="home-container home-container--first-top-short">
                   <div class="home-container_section home-container_section--top-long">
    <h2 class="u-text-centered">ReviewR</h2>
    <div class="location-bar">
    <div class="tab-nav-container">
        <ul class="tab-nav js-tab-nav tab-nav--center js-tab-nav--center">
                
        <li class="tab-nav_item">
    </div>
</div>
                    <div class="home-container_section js-hot-new-businesses-section">
            <h3 class="explore-city_title">Search Results</h3>
<div class="arrange arrange--wrap arrange--3-units arrange--30">
<% for(var i=0; i<newbusiness.length; i++) {%>

    <div class="arrange_unit">
        <div class="card business-passport-card">
<div class="card_photo">   
<div class="photo-box photo-box--interactive photo-box--background" style="background-image: url(https://s3-media2.fl.yelpcdn.com/bphoto/uBHu1TbXnTbudFOHvd9uKg/l.jpg)">
    <a href="/biz/king-almond-irvine" class="photo-box_link js-analytics-click" data-analytics-label="biz-photo">
    <img alt="King Almond" class="photo-box-img" height="400" src="https://s3-media2.fl.yelpcdn.com/bphoto/uBHu1TbXnTbudFOHvd9uKg/l.jpg" srcset="https://s3-media2.fl.yelpcdn.com/bphoto/uBHu1TbXnTbudFOHvd9uKg/o.jpg 1.67x" width="600">

</a>

</div>

</div>
<div class="card_body">
<div class="card_content">
    <h3 class="card_content-title--linked u-text-truncate">
            
<div class="business-link-hovercard js-business-link-hovercard">
<span class="business-link-hovercard_business-link js-business-link">
        <a class="biz-name js-analytics-click" data-analytics-label="biz-name" href="/view-business/<%= newbusiness[i].id %>" data-hovercard-id="" ><span ><%= newbusiness[i].name %></span></a>

</span>


<div class="js-business-hovercard u-break-word business-link-hovercard_hovercard u-hidden">
<div class="media-block media-block--12 biz-listing-medium">
<div class="media-avatar">
            <div class="photo-box pb-90s">
    <a href="/view-business/<%= newbusiness[i].id %>" class="js-analytics-click" data-analytics-label="biz-photo">
    <img alt="King Almond" class="photo-box-img" height="90" src="https://s3-media2.fl.yelpcdn.com/bphoto/uBHu1TbXnTbudFOHvd9uKg/90s.jpg" srcset="https://s3-media2.fl.yelpcdn.com/bphoto/uBHu1TbXnTbudFOHvd9uKg/ms.jpg 1.11x,https://s3-media2.fl.yelpcdn.com/bphoto/uBHu1TbXnTbudFOHvd9uKg/ls.jpg 2.78x,https://s3-media2.fl.yelpcdn.com/bphoto/uBHu1TbXnTbudFOHvd9uKg/120s.jpg 1.33x,https://s3-media2.fl.yelpcdn.com/bphoto/uBHu1TbXnTbudFOHvd9uKg/168s.jpg 1.87x,https://s3-media2.fl.yelpcdn.com/bphoto/uBHu1TbXnTbudFOHvd9uKg/258s.jpg 2.87x,https://s3-media2.fl.yelpcdn.com/bphoto/uBHu1TbXnTbudFOHvd9uKg/180s.jpg 2.00x" width="90">

</a>

</div>



</div>
<div class="media-story">
    <div class="media-title clearfix">
        <a class="biz-name js-analytics-click" data-analytics-label="biz-name" href="/biz/king-almond-irvine" data-hovercard-id="" ><span >King Almond</span></a>

</div>
<div class="biz-rating biz-rating-large clearfix" itemprop="aggregateRating" itemscope itemtype="http://schema.org/AggregateRating">
    



<div class="i-stars i-stars--regular-<%= Math.floor(newbusiness[i].rating) %> rating-large" title="5.0 star rating">
<img class="offscreen" height="303" src="https://s3-media2.fl.yelpcdn.com/assets/srv0/yelp_design_web/9b34e39ccbeb/assets/img/stars/stars.png" width="84" alt="5.0 star rating">
</div>
<meta itemprop="ratingValue" content="5.0">


        <span class="review-count rating-qualifier">
<span itemprop="reviewCount"><%= newbusiness[i].numreviews %></span> reviews
</span>

</div>

<div class="price-category">
        <span class="category-str-list">
        Bakeries,
        Specialty Food,
        Desserts
</span>


</div>

<small>
<span class="addr-city">Irvine, CA</span>

</small>

</div>
</div>

</div>

</div>

    </h3>
            <div class="biz-rating biz-rating-large clearfix" >
    



<div class="i-stars i-stars--regular-<%= Math.floor(newbusiness[i].rating) %>  rating-large" title="5.0 star rating">
<img class="offscreen" height="303" src="https://s3-media2.fl.yelpcdn.com/assets/srv0/yelp_design_web/9b34e39ccbeb/assets/img/stars/stars.png" width="84" alt="5.0 star rating">
</div>


        <span class="review-count rating-qualifier">
<%= newbusiness[i].numreviews %> reviews
</span>

</div>

        <div class="price-category">
        <span class="category-str-list">
        <%= newbusiness[i].category1 %>,
        <%= newbusiness[i].category2 %>
</span>


</div>

<div class="city-state u-text-truncate u-space-t1 u-space-b1">
        <span class="addr-city"><%= newbusiness[i].city %>,<%= newbusiness[i].state %></span>

</div>
</div>
</div>
</div>

</div>
 <% } %>       

     

<div class="u-space-t2 u-text-centered">
    <a class="js-analytics-click explore-city_see-all" data-analytics-label="link-more" href="/search?attrs=NewBusiness" >See more hot and new businesses</a>
</div>

    </div>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.3/umd/popper.min.js" integrity="sha384-vFJXuSJphROIrBnz7yo7oB41mKfc8JzQZiCq4NCceLEaO4IHwicKwpJf9c9IpFgh" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.2/js/bootstrap.min.js" integrity="sha384-alpBpkh1PFOepccYVYDB4do5UnbKysX5WZXm3XxPqe5iKTfUKjNkCk9SaVuEZflJ" crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

</body>
</html>
