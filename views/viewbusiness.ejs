                    <script>
          
  $("#bookmark").on("click",function(e) {
    e.preventDefault(); // cancel the link itself
    if ($("#checktext").text() == 'Bookmark'){
      $.ajax({
        type:"POST",
        url:"/users/bookmark/"+$('#hiddenvalue').val(), 
     }).done(function(json){
      $("#checktext").text("Remove Bookmark")
     })
    }else{
      $.ajax({
        type:"POST",
        url:"/users/deletebookmark/", 
        data:{
          businessid:$('#hiddenvalue').val(),
        }
     }).done(function(json){
      $("#checktext").text("Bookmark");
     })
    }
  });
                    </script>
